model world_pvptype {
  id          Int           @id @default(autoincrement())
  name        String        @db.VarChar(255)
  clientType  Int           @default(0) @db.UnsignedTinyInt
  serverType  String        @db.VarChar(255) @default("no-pvp")  // ['no-pvp', 'pvp', 'pvp-enforced']
